{
    "kind": "Ingress",
    "apiVersion": "networking.k8s.io/v1",
    "metadata": {
      "name": "coder",
      "namespace": "coder",
      "labels": {
        "app.kubernetes.io/instance": "coder"
      },
      "annotations": {
        "alb.ingress.kubernetes.io/scheme": "internet-facing",
        "alb.ingress.kubernetes.io/target-type": "ip"
      }
    },
    "spec": {
      "ingressClassName": "alb",
      "rules": [
        {
          "http": {
            "paths": [
              {
                "path": "/",
                "pathType": "Prefix",
                "backend": {
                  "service": {
                    "name": "coder",
                    "port": {
                      "number": 80
                    }
                  }
                }
              }
            ]
          }
        }
      ]
    }
  }